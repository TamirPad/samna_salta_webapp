{
  "name": "@samna-salta/frontend",
  "version": "1.0.0",
  "description": "Samna Salta - Frontend Application",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@samna-salta/common": "file:../../packages/common",
    "@stripe/stripe-js": "^2.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "ajv": "^8.17.1",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-icons": "^5.5.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.1",
    "web-vitals": "^3.5.0",
    "zustand": "^4.4.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "ANALYZE=true npm run build",
    "test": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 50",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings 50 --format=compact",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prebuild": "echo 'Skipping type-check for cleanup'",
    "precommit": "npm run lint && npm run type-check && npm run test",
    "deploy:render": "npm run build && echo 'Build completed successfully'",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:size": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode=static --report",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules package-lock.json && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "react/jsx-key": "error",
      "react/jsx-no-duplicate-props": "error",
      "react/jsx-no-undef": "error",
      "react/no-array-index-key": "warn",
      "react/no-unescaped-entities": "warn",
      "react/self-closing-comp": "warn",
      "no-console": "warn",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.1.1",
    "react-app-rewired": "^2.2.1",
    "ts-jest": "^29.4.0",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/samna-salta-webapp.git"
  },
  "keywords": [
    "react",
    "typescript",
    "redux",
    "food-ordering",
    "yemenite",
    "restaurant"
  ],
  "author": "Your Name",
  "license": "MIT"
}
